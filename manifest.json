{
  "vendor": "qualitydigital",
  "name": "service-example",
  "version": "0.0.0",
  "title": "Service Example",
  "description": "Reference app for VTEX IO Services",
  "mustUpdateAt": "2024-11-06",
  "builders": {
    "node": "7.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "httpstat.us",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "jsonplaceholder.typicode.com",
        "path": "/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "acctglobal.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "vbase-read-only"
    },
    {
      "name": "vbase-read-write"
    }
  ],
  "settingsSchema": {
    "title": "getProductByID",
    "description": "Get products by id on vtex catalog API",
    "type": "object",
    "properties": {
      "appKeyGetProductByID": {
        "type": "string",
        "title": "App Key",
        "description": "Get your app key at https://developers.vtex.com/docs/guides/getting-an-app-key",
        "default": ""
      },
      "appTokenGetProductByID": {
        "type": "string",
        "title": "App Token for auth",
        "description": "Get your token at https://developers.vtex.com/docs/guides/getting-an-app-token",
        "default": ""
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
